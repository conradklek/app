{
	"openapi": "3.0.0",
	"info": {
		"title": "app",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "https://app-cklek.vercel.app"
		}
	],
	"paths": {
		"/api": {
			"post": {
				"summary": "Spawn a new agent that can be used to solve a task.",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"prompt": {
										"type": "string"
									}
								},
								"required": ["prompt"]
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Agent created successfully."
					}
				}
			}
		}
	}
}
