{
	"openapi": "3.0.0",
	"info": {
		"title": "app",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "https://app-cklek.vercel.app"
		}
	],
	"paths": {
		"/api": {
			"get": {
				"summary": "Responds with hello plus the name provided in the query string",
				"parameters": [
					{
						"name": "name",
						"in": "query",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		},
		"/ai/agent": {
			"get": {
				"summary": "Creates an agent with a calculator tool to solve the prompt provided in the query string",
				"parameters": [
					{
						"name": "prompt",
						"in": "query",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				]
			}
		}
	}
}
