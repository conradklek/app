{
	"openapi": "3.0.0",
	"info": {
		"title": "MongoDB Data API",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "https://data.mongodb-api.com/app/data-zfwhk/endpoint/data/v1"
		}
	],
	"paths": {
		"/action/insertOne": {
			"post": {
				"summary": "Insert a document",
				"operationId": "insertOne",
				"parameters": [
					{
						"name": "apiKey",
						"in": "header",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"dataSource": {
										"type": "string"
									},
									"database": {
										"type": "string"
									},
									"collection": {
										"type": "string"
									},
									"document": {
										"type": "object",
										"properties": {
											"name": {
												"type": "string"
											}
										},
										"required": ["name"]
									}
								},
								"required": ["dataSource", "database", "collection", "document"]
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Document inserted successfully"
					}
				}
			}
		},
		"/action/findOne": {
			"post": {
				"summary": "Find a document",
				"operationId": "findOne",
				"parameters": [
					{
						"name": "apiKey",
						"in": "header",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"dataSource": {
										"type": "string"
									},
									"database": {
										"type": "string"
									},
									"collection": {
										"type": "string"
									},
									"filter": {
										"type": "object",
										"properties": {
											"name": {
												"type": "string"
											}
										},
										"required": ["name"]
									}
								},
								"required": ["dataSource", "database", "collection", "filter"]
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Document found successfully"
					}
				}
			}
		},
		"/action/deleteOne": {
			"post": {
				"summary": "Delete a document",
				"operationId": "deleteOne",
				"parameters": [
					{
						"name": "apiKey",
						"in": "header",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"dataSource": {
										"type": "string"
									},
									"database": {
										"type": "string"
									},
									"collection": {
										"type": "string"
									},
									"filter": {
										"type": "object",
										"properties": {
											"name": {
												"type": "string"
											}
										},
										"required": ["name"]
									}
								},
								"required": ["dataSource", "database", "collection", "filter"]
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Document deleted successfully"
					}
				}
			}
		}
	}
}
