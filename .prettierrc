{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 1000,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"semi": false,
	"tabWidth": 2,
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "auto"
}
